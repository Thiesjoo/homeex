{
	"name": "HomeEx",
	"version": "0.2.0",
	"description": "This extension sets your homepage to thies.dev/home, my custom developed home page.",
	"manifest_version": 3,
	"author": "Thies Nieborg",
	"host_permissions": ["https://pos.svia.nl/*", "https://*.thies.dev/"],
	"permissions": ["webRequest", "webNavigation", "storage", "cookies"],
	"background": {
		"service_worker": "background.js"
	},
	"options_ui": {
		"page": "options/options.html",
		"open_in_tab": false
	},
	"action": {
		"default_popup": "options/options.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"icons": {
		"128": "icon.png"
	},
	"key": ""
}
